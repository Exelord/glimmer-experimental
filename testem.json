{
  "framework": "qunit",
  "test_page": "tests.html",
  "launchers": {
    "Node": {
      "command": "node_modules/.bin/qunit dist/nodeTests.bundle.js",
      "protocol": "tap"
    }
  },
  "on_start": {
    "command": "webpack --watch",
    "wait_for_text": "Built at:"
  },
  "watch_files": [
    "dist/tests.bundle.js",
    "dist/nodeTests.bundle.js"
  ],
  "launch_in_dev": [
    "Node"
  ]
}
